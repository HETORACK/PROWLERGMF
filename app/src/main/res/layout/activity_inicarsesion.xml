<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo7"
    tools:context=".Inicarsesion">

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="355dp"
        android:layout_height="60dp"
        android:layout_marginTop="216dp"
        android:background="@drawable/eddbackground2"
        android:drawableLeft="@drawable/baseline_directions_run_24"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/typoroundbolddemo"
        android:hint="Usuario"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColor="#F1F8F9"
        android:textColorHint="#CDCDCD"
        android:textSize="20sp"
        android:theme="@style/campodetextoblanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/txtepasword3"
        android:layout_width="355dp"
        android:layout_height="60dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/eddbackground2"
        android:drawableLeft="@drawable/baseline_lock_24"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fontFamily="@font/typoroundbolddemo"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:textColor="#F1F8F9"
        android:textColorHint="#CDCDCD"
        android:textSize="20sp"
        android:theme="@style/campodetextoblanco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName7" />

    <Button
        android:id="@+id/button3"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="234dp"
        android:layout_height="49dp"
        android:layout_marginBottom="164dp"
        android:fontFamily="@font/typoroundbolddemo"
        android:gravity="left"
        android:onClick="olvidar"
        android:text="Olvidé mi contraseña"
        android:textColor="#59828F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/typoroundbolddemo"
        android:text="INICIAR SESION"
        android:textColor="#F1F8F9"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/prowler" />

    <ImageButton
        android:id="@+id/imageButton8"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="porfin"
        android:onClick="ingresar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/sig2" />

</androidx.constraintlayout.widget.ConstraintLayout>